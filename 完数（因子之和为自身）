#include <iostream>
using namespace std;
void isperfect()
{
for(int a=1;a<=1000;a++)
{
    int sum=0;
    for(int b=1;b<a;b++)
    {
        
        if(a%b==0)
            sum+=b;
    }
        if(sum==a)
            {cout<<a<<" is a perfect number."<<endl;
            for(int b=1;b<a;b++)
    {
        
        if(a%b==0)
           cout<<b<<" ";
    }
        cout<<endl;
            }
    
}
}

int main()
{
isperfect();
}
